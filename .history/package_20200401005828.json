{
  "name": "minimal-express",
  "version": "0.0.0",
  "about": "Simple Nodejs Express Package",
  "port": 3000,
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "line-reader": "^0.4.0",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.0",
    "node-persist": "^3.0.5",
    "node-sass": "^4.13.1",
    "pug": "2.0.4",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/compression": "0.0.36",
    "@types/dotenv": "^6.1.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.12",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.17",
    "@types/shelljs": "^0.8.0",
    "@types/supertest": "^2.0.7",
    "concurrently": "^5.0.2",
    "install": "^0.13.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.1"
  }
}
